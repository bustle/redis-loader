{
  "name": "redis-loader",
  "version": "6.3.2",
  "description": "An io-redis like object that batches queries with dataloader",
  "author": "Tristan Siegel <tristan.siegel@bustle.com>",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "keywords": [
    "async/await",
    "dataloader",
    "ioredis",
    "multi",
    "pipeline",
    "redis",
    "typescript"
  ],
  "main": "dist",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf dist/*",
    "lint": "eslint --ignore-path .gitignore src/**/*.ts && tsc",
    "format": "eslint --ignore-path .gitignore src/**/*.ts --fix",
    "prepare": "npm run build",
    "test": "jest --runInBand --bail --coverage"
  },
  "dependencies": {
    "dataloader": "^2.1.0",
    "redis-commands": "^1.4.0"
  },
  "peerDependencies": {
    "ioredis": ">=4.0.0"
  },
  "devDependencies": {
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-promise": "^6.0.0",
    "ioredis": "^4.28.5",
    "jest": "^28.1.0",
    "streaming-iterables": "^7.0.4",
    "ts-jest": "^28.0.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}

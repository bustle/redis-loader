{
  "name": "redis-loader",
  "version": "6.3.2",
  "description": "An io-redis like object that batches queries with dataloader",
  "author": "Tristan Siegel <tristan.siegel@bustle.com>",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "keywords": [
    "async/await",
    "dataloader",
    "ioredis",
    "multi",
    "pipeline",
    "redis",
    "typescript"
  ],
  "main": "dist",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf dist/*",
    "lint": "eslint --ignore-path .gitignore src/**/*.ts && tsc",
    "format": "eslint --ignore-path .gitignore src/**/*.ts --fix",
    "prepare": "npm run build",
    "test": "jest --runInBand --bail --coverage"
  },
  "dependencies": {
    "bluestream": "10",
    "dataloader": "^1.4.0",
    "redis-commands": "^1.4.0"
  },
  "peerDependencies": {
    "ioredis": ">=3.0.0"
  },
  "devDependencies": {
    "@types/ioredis": "^3.2.14",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "bluebird": "^3.5.3",
    "chai": "^4.1.2",
    "eslint": "^8.14.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.20.2",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-typescript": "^0.14.0",
    "ioredis": "^4.2.0",
    "jest": "^28.1.0",
    "streaming-iterables": "^3.0.7",
    "ts-jest": "^28.0.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^4.6.4",
    "typescript-eslint-parser": "^18.0.0"
  }
}

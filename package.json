{
  "name": "redis-loader",
  "version": "4.0.0",
  "description": "An io-redis like object that batches queries with dataloader",
  "author": "Tristan Siegel <tristan.siegel@bustle.com>",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "main": "dist",
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rm -rf dist/*",
    "docs": "rm -rf ./docs && typedoc --out ./docs/ --theme minimal src/*.ts && touch ./docs/.nojekyll",
    "lint": "eslint --ignore-path .gitignore src/**/*.ts && tsc",
    "format": "eslint --ignore-path .gitignore src/**/*.ts test/**/*.ts --fix",
    "prepublish": "yarn build",
    "test": "jest --runInBand --bail --coverage"
  },
  "dependencies": {
    "bluestream": "10",
    "dataloader": "^1.3.0",
    "invariant": "^2.2.4",
    "redis-commands": "^1.3.5"
  },
  "peerDependencies": {
    "ioredis": "^3.0.0"
  },
  "devDependencies": {
    "@types/async": "^2.0.45",
    "@types/errorhandler": "0.0.32",
    "@types/ioredis": "^3.2.5",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.6",
    "bluebird": "^3.5.1",
    "chai": "^4.1.2",
    "eslint": "^5.3.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-typescript": "^0.12.0",
    "ioredis": "^3.0.0",
    "jest": "^23.4.2",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.0",
    "tslint": "^5.8.0",
    "typedoc": "^0.11.1",
    "typescript": "^3.0.1",
    "typescript-eslint-parser": "^17.0.1"
  }
}
